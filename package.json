{
  "name": "ExploreASLJS",
  "productName": "ExploreASLJS",
  "version": "0.3.0",
  "description": "A user interface for the analysis of arterial spin labeling (ASL) images",
  "repository": "https://github.com/MauricePasternak/ExploreASLJS",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "Maurice Pasternak",
    "email": "maurice.pasternak@utoronto.ca"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "ExploreASLJS",
        "executableName": "ExploreASLJS",
        "platform": [
          "darwin",
          "linux",
          "win32"
        ],
        "icon": "src/assets/appIcons/ExploreASLIcon",
        "osxSign": {
          "hardened-runtime": true,
          "entitlements": "entitlements.plist",
          "entitlements-inherit": "entitlements.plist",
          "signature-flags": "library"
        },
        "osxNotarize": {
          "appleId": "mpasternak@this.is.a.placeholder.com",
          "appleIdPassword": "this.is.a.placeholder.password"
        }
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "ExploreASLJS",
            "iconUrl": "https://raw.githubusercontent.com/MauricePasternak/ExploreASLJS/main/src/assets/appIcons/ExploreASLIcon.ico",
            "setupIcon": "src/assets/appIcons/ExploreASLIcon.ico"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "description": "A user interface for analyzing arterial-spin labelled images with ExploreASL",
            "icon": "src/assets/appIcons/ExploreASLIcon.png",
            "maintainer": "Maurice Pasternak",
            "name": "ExploreASLJS",
            "productName": "ExploreASLJS",
            "categories": [
              "Science"
            ]
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "icon": "src/assets/appIcons/ExploreASLIcon.png",
            "overwrite": true
          }
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "owner": "MauricePasternak",
            "repo": "ExploreASLJS",
            "prerelease": true
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "devServer": {
              "liveReload": false
            },
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/preload.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.59",
    "@electron-forge/maker-deb": "^6.0.0-beta.59",
    "@electron-forge/maker-dmg": "6.0.0-beta.59",
    "@electron-forge/maker-rpm": "^6.0.0-beta.59",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.59",
    "@electron-forge/maker-zip": "^6.0.0-beta.59",
    "@electron-forge/plugin-webpack": "6.0.0-beta.64",
    "@faker-js/faker": "^7.4.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@svgr/webpack": "^6.2.1",
    "@types/colormap": "^2.3.1",
    "@types/cross-spawn": "^6.0.2",
    "@types/dataframe-js": "^1.4.0",
    "@types/dot-object": "^2.1.2",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.178",
    "@types/normalize-path": "^3.0.0",
    "@types/papaparse": "^5.3.3",
    "@types/quill": "^2.0.9",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-plotly.js": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vercel/webpack-asset-relocator-loader": "1.7.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.0.0",
    "electron": "19.0.8",
    "eslint": "^8.19.0",
    "eslint-plugin-import": "^2.20.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "node-loader": "^2.0.0",
    "react-refresh": "^0.14.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@hookform/resolvers": "^2.9.5",
    "@mui/icons-material": "^5.4.1",
    "@mui/lab": "^5.0.0-alpha.89",
    "@mui/material": "^5.8.7",
    "@nivo/core": "^0.79.0",
    "@nivo/heatmap": "^0.79.1",
    "@nivo/scatterplot": "^0.79.1",
    "@nivo/swarmplot": "^0.79.1",
    "chokidar": "^3.5.3",
    "colormap": "^2.3.2",
    "cross-spawn": "^7.0.3",
    "data-forge": "^1.9.2",
    "data-forge-fs": "^0.0.9",
    "dot-object": "^2.1.4",
    "electron-squirrel-startup": "^1.0.0",
    "formik": "^2.2.9",
    "jotai": "^1.7.4",
    "lodash": "^4.17.21",
    "nifti-stream": "^0.2.1",
    "pathlib-js": "^1.4.2",
    "quill": "^1.3.7",
    "react": "^18.2.0",
    "react-data-grid": "^7.0.0-beta.15",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.33.1",
    "tree-kill": "^1.2.2",
    "use-debounce": "^8.0.1",
    "yup": "^0.32.11"
  }
}
